ess(\"保存成功:)\");\n\n          _this3.dialogFormVisible = false;\n\n          _this3.load();\n        } else {\n          _this3.$message.error(\"保存失败:(\");\n        }\n      });\n    },\n    reset: function reset() {\n      this.meal_id = \"\";\n      this.meal_name = \"\";\n      this.load();\n    },\n    handleSizeChange: function handleSizeChange(pageSize) {\n      console.log(\"\\u6BCF\\u9875 \".concat(pageSize, \" \\u6761\"));\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      console.log(\"\\u5F53\\u524D\\u9875: \".concat(pageNum));\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyFA;EACAA,gBADA;EAEAC,IAFA,kBAEA;IACA;MACAC,aADA;MAEAC,QAFA;MAGAC,UAHA;MAIAC,WAJA;MAKAC,UALA;MAMAC,WANA;MAOAC,aAPA;MAQAC,wBARA;MASAC;IATA;EAWA,CAdA;EAeAC,OAfA,qBAeA;IACA;EACA,CAjBA;EAkBAC;IACAC,IADA,kBACA;MAAA;;MACA;MACA;QACAC;UACAT,uBADA;UAEAD,qBAFA;UAGAE,qBAHA;UAIAC,qBAJA;UAKAC;QALA;MADA,GAQAO,IARA,CAQA;QACAC;QACA;QACA;MACA,CAZA;IAaA,CAhBA;IAiBAC,aAjBA,2BAiBA;MACA;MACA;IACA,CApBA;IAqBAC,UArBA,sBAqBAC,GArBA,EAqBA;MACA;MACA;IACA,CAxBA;IAyBAC,UAzBA,sBAyBAC,EAzBA,EAyBA;MAAA;;MACA;QACA;UACA;;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CAlCA;IAmCAC,IAnCA,kBAmCA;MAAA;;MAAA;MACA;QACA;UACA;;UACA;;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;IASA,CA7CA;IA8CAC,KA9CA,mBA8CA;MACA;MACA;MACA;IACA,CAlDA;IAmDAC,gBAnDA,4BAmDAnB,QAnDA,EAmDA;MACAW;MACA;MACA;IACA,CAvDA;IAwDAS,mBAxDA,+BAwDArB,OAxDA,EAwDA;MACAY;MACA;MACA;IACA;EA5DA;AAlBA","names":["name","data","tableData","total","pageNum","pageSize","address","meal_id","meal_name","dialogFormVisible","form","created","methods","load","params","then","console","handleNewMeal","handleEdit","row","deleteMeal","id","save","reset","handleSizeChange","handleCurrentChange"],"sourceRoot":"src/views","sources":["MainPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 20px; height: 10px\">\r\n      <el-breadcrumb separator=\"/\">\r\n        <el-breadcrumb-item :to=\"{ path: '/' }\" class=\"el-icon-s-home\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item class=\"el-icon-s-custom\"><a href=\"/\">个人界面</a></el-breadcrumb-item>\r\n        <el-breadcrumb-item class=\"el-icon-thumb\">登录</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0; height: 25px\">\r\n      <el-input style=\"width: 140px\" placeholder=\"请输入餐品ID\" suffix-icon=\"el-icon-date\" v-model=\"meal_id\"></el-input>\r\n      <el-input style=\"width: 140px; margin-left: 5px\" placeholder=\"请输入餐品名\" suffix-icon=\"el-icon-food\" v-model=\"meal_name\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleNewMeal\">新增<i class=\"el-icon-circle-plus-outline\" style=\"margin-left: 5px\"></i></el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border: stripe>\r\n      <el-table-column prop=\"meal_id\" label=\"ID\" width=\"100\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"meal_name\" label=\"餐品名\" width=\"500\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"likes_num\" label=\"点赞数\" width=\"140\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"comments_num\" label=\"评论数\" width=\"140\" align=\"center\">\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"操作\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\"><i class=\"el-icon-view\"></i> </el-button>\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\"><i class=\"el-icon-edit-outline\"></i></el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 5px\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='取消'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确认删除吗？\"\r\n              @confirm=\"deleteMeal(scope.row.meal_id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\"><i class=\"el-icon-delete\"></i> </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--        页码-->\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[5, 10, 15, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <!--        对话框-->\r\n    <el-dialog title=\"新增餐品\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n      <el-form label-width=\"69px\" size=\"small\">\r\n        <el-form-item label=\"餐品名\">\r\n          <el-input v-model=\"form.meal_name\" autocomplete=\"off\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"食堂\">\r\n          <el-select v-model=\"form.region\" placeholder=\"请选择食堂\">\r\n            <el-option label=\"鼓楼一食堂\" value=\"1\"></el-option>\r\n            <el-option label=\"鼓楼二食堂\" value=\"2\"></el-option>\r\n            <el-option label=\"仙林四食堂\" value=\"4\"></el-option>\r\n            <el-option label=\"仙林五食堂\" value=\"5\"></el-option>\r\n            <el-option label=\"仙林六食堂\" value=\"6\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"MainPage\",\r\n  data(){\r\n    return{\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      address: 1,\r\n      meal_id: \"\",\r\n      meal_name: \"\",\r\n      dialogFormVisible:false,\r\n      form:{},\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    load(){\r\n      //请求分页查询数据(meal_id,meal_name,likes_num,comments_num\r\n      this.request.get(\"/meal/page\",{\r\n        params:{\r\n          pageSize:this.pageSize,\r\n          pageNum:this.pageNum,\r\n          address:this.address,\r\n          meal_id:this.meal_id,\r\n          meal_name:this.meal_name,\r\n        }\r\n      }).then(res =>{\r\n        console.log(res)\r\n        this.tableData = res.records\r\n        this.total = res.total\r\n      })\r\n    },\r\n    handleNewMeal(){\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n    },\r\n    handleEdit(row){\r\n      this.form = row\r\n      this.dialogFormVisible = true\r\n    },\r\n    deleteMeal(id){\r\n      this.request.delete(\"/meal/\"+id).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"删除成功:)\")\r\n          this.load()\r\n        }else {\r\n          this.$message.error(\"删除失败:(\")\r\n        }\r\n      })\r\n    },\r\n    save(){//向后台发送新增和编辑数据\r\n      this.request.post(\"/meal\",this.form).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"保存成功:)\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else {\r\n          this.$message.error(\"保存失败:(\")\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.meal_id=\"\"\r\n      this.meal_name=\"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(`每页 ${pageSize} 条`);\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(`当前页: ${pageNum}`);\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}