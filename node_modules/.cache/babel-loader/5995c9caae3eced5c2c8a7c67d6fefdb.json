\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleNewMeal\">新增<i class=\"el-icon-circle-plus-outline\" style=\"margin-left: 5px\"></i></el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border: stripe>\r\n      <el-table-column prop=\"meal_id\" label=\"餐品ID\" width=\"100\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"meal_name\" label=\"餐品名\" width=\"600\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"score\" label=\"平均评分\" width=\"140\" align=\"center\">\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"操作\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"handleScore(scope.row)\"><i class=\"el-icon-edit-outline\"></i></el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 5px\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='取消'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确认删除吗？\"\r\n              @confirm=\"deleteMeal(scope.row.meal_id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\"><i class=\"el-icon-delete\"></i> </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--        页码-->\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[5, 10, 15, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <!--        对话框-->\r\n    <el-dialog title=\"新增餐品\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n      <el-form label-width=\"69px\" size=\"small\">\r\n        <el-form-item label=\"餐品名\">\r\n          <el-input v-model=\"form.meal_name\" autocomplete=\"off\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"食堂\">\r\n          <el-select v-model=\"form.region\" placeholder=\"请选择食堂\">\r\n            <el-option label=\"鼓楼一食堂\" value=\"1\"></el-option>\r\n            <el-option label=\"鼓楼二食堂\" value=\"2\"></el-option>\r\n            <el-option label=\"仙林四食堂\" value=\"4\"></el-option>\r\n            <el-option label=\"仙林五食堂\" value=\"5\"></el-option>\r\n            <el-option label=\"仙林六食堂\" value=\"6\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"餐品评分\" :visible.sync=\"dialogScoreVisible\" width=\"30%\">\r\n      <el-form label-width=\"69px\" size=\"small\">\r\n        <el-form-item label=\"评分\">\r\n          <el-select v-model=\"score\" placeholder=\"请评分\">\r\n            <el-option label=\"1\" value=\"1\"></el-option>\r\n            <el-option label=\"2\" value=\"2\"></el-option>\r\n            <el-option label=\"3\" value=\"3\"></el-option>\r\n            <el-option label=\"4\" value=\"4\"></el-option>\r\n            <el-option label=\"5\" value=\"5\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogScoreVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveScore\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"MainPage\",\r\n  data(){\r\n    return{\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      address: 2,\r\n      meal_id: \"\",\r\n      meal_name: \"\",\r\n      dialogFormVisible:false,\r\n      dialogScoreVisible:false,\r\n      form:{},\r\n      score: \"\",\r\n\r\n      userid: \"\",\r\n      username: \"\",\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    load(){\r\n      //请求分页查询数据(meal_id,meal_name,userid,平均score\r\n      this.request.get(\"/meal/page\",{\r\n        params:{\r\n          pageSize:this.pageSize,\r\n          pageNum:this.pageNum,\r\n          address:this.address,\r\n          meal_id:this.meal_id,\r\n          meal_name:this.meal_name,\r\n        }\r\n      }).then(res =>{\r\n        console.log(res)\r\n        this.tableData = res.records\r\n        this.total = res.total\r\n      })\r\n    },\r\n    handleNewMeal(){\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n    },\r\n    handleScore(row){\r\n      this.form = row\r\n      this.dialogScoreVisible = true\r\n    },\r\n    deleteMeal(meal){//向后台请求删除数据,参数:meal_id\r\n      this.request.delete(\"/meal/\"+meal).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"删除成功:)\")\r\n          this.load()\r\n        }else {\r\n          this.$message.error(\"删除失败:(\")\r\n        }\r\n      })\r\n    },\r\n    save(){//向后台发送新增和编辑数据,参数:form.meal_name,form.region(address)\r\n      this.request.post(\"/meal\",this.form).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"保存成功:)\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else {\r\n          this.$message.error(\"保存失败:(\")\r\n        }\r\n      })\r\n    },\r\n    saveScore(){//向后台发送得分请求,参数:form.meal_name,form.region(address),score\r\n      this.request.post(\"/meal\",this.score,this.form).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"保存成功:)\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        }else {\r\n          this.$message.error(\"保存失败:(\")\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.meal_id=\"\"\r\n      this.meal_name=\"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(`每页 ${pageSize} 条`);\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(`当前页: ${pageNum}`);\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}