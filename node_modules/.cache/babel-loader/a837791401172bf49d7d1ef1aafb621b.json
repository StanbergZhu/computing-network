{"ast":null,"code":"import \"core-js/modules/es.array.fill.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: 'HomeView',\n  data: function data() {\n    var item = {\n      date: '2016-05-02',\n      name: '王小虎',\n      dish: '上海市普陀区金沙江路 1518 弄'\n    };\n    return {\n      tableData: Array(10).fill(item),\n      collapseBtnClass: 'el-icon-s-fold',\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShow: true\n    };\n  },\n  methods: {\n    collapse: function collapse() {\n      //点击收缩菜单栏按钮触发\n      this.isCollapse = !this.isCollapse;\n\n      if (this.isCollapse) {\n        //收缩\n        this.sideWidth = 64;\n        this.collapseBtnClass = 'el-icon-s-unfold';\n        this.logoTextShow = false;\n      } else {\n        //展开\n        this.sideWidth = 200;\n        this.collapseBtnClass = 'el-icon-s-fold';\n        this.logoTextShow = true;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoHA;EACAA,gBADA;EAEAC,IAFA,kBAEA;IACA;MACAC,kBADA;MAEAF,WAFA;MAGAG;IAHA;IAKA;MACAC,+BADA;MAEAC,kCAFA;MAGAC,iBAHA;MAIAC,cAJA;MAKAC;IALA;EAOA,CAfA;EAgBAC;IACAC,QADA,sBACA;MAAA;MACA;;MACA;QAAA;QACA;QACA;QACA;MACA,CAJA,MAIA;QAAA;QACA;QACA;QACA;MACA;IACA;EAZA;AAhBA","names":["name","data","date","dish","tableData","collapseBtnClass","isCollapse","sideWidth","logoTextShow","methods","collapse"],"sourceRoot":"src/views","sources":["HomeView.vue"],"sourcesContent":["<template>\n  <el-container style=\"min-height: 100vh;\">\n    <el-aside width=\"sideWidth + 'px'\" style=\"background-color: rgb(238, 241, 246); box-shadow: 2px 0 6px rgb(0 21 41/40%)\">\n      <el-menu :default-openeds=\"['1', '3']\" style=\"min-height: 100%; overflow-x: hidden\"\n               background-color=\"rgb(48,65,86)\"\n               text-color=\"#fff\"\n               active-text-color=\"#ffd04b\"\n               :collapse-transition=\"false\"\n               :collapse=\"isCollapse\"\n      >\n        <div style=\"height: 60px; line-height: 60px; text-align: center\">\n<!--          //此处图片需要修改-->\n          <img src=\"../assets/logo.png\" alt=\"\" style=\"width: 20px; position: relative; top: 5px; margin-right: 6px\">\n\n          <b style=\"color: white\" v-show=\"logoTextShow\">南大食堂餐品评价</b>\n        </div>\n\n          <el-submenu index=\"1\">\n            <template slot=\"title\">\n              <i class=\"el-icon-school\"></i>\n              <span slot=\"title\">鼓楼校区</span>\n            </template>\n\n            <el-menu-item index=\"1-1\">一食堂</el-menu-item>\n            <el-menu-item index=\"1-2\">二食堂</el-menu-item>\n\n          </el-submenu>\n\n          <el-submenu index=\"2\">\n            <template slot=\"title\">\n              <i class=\"el-icon-school\"></i>\n              <span slot=\"title\">仙林校区</span>\n            </template>\n\n            <el-menu-item index=\"2-1\">四食堂</el-menu-item>\n            <el-menu-item index=\"2-2\">五食堂</el-menu-item>\n            <el-menu-item index=\"2-3\">六食堂</el-menu-item>\n\n          </el-submenu>\n\n        </el-menu>\n    </el-aside>\n\n    <el-container>\n      <el-header style=\"font-size: 12px; border-bottom: 1px solid #ccc; line-height: 60px; display: flex\">\n        <div style=\"flex: 1; font-size: 25px\">\n          <span :class=\"collapseBtnClass\" style=\"cursor: pointer\" @click=\"collapse\"></span>\n        </div>\n        <el-dropdown style=\"width: 70px; cursor: pointer\">\n          <span>王小虎</span>\n          <i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\n          <el-dropdown-menu slot=\"dropdown\">\n            <el-dropdown-item>个人信息</el-dropdown-item>\n            <el-dropdown-item>退出账号</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n\n      </el-header>\n\n      <el-main>\n        <div style=\"margin-bottom: 30px\">\n          <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item :to=\"{ path: '/' }\" class=\"el-icon-s-home\">首页</el-breadcrumb-item>\n            <el-breadcrumb-item class=\"el-icon-s-custom\"><a href=\"/\">个人界面</a></el-breadcrumb-item>\n            <el-breadcrumb-item class=\"el-icon-thumb\">登录界面</el-breadcrumb-item>\n          </el-breadcrumb>\n        </div>\n        <div style=\"margin: 10px 0\">\n          <el-input style=\"width: 140px\" placeholder=\"请输入日期\" suffix-icon=\"el-icon-date\"></el-input>\n          <el-input style=\"width: 140px; margin-left: 5px\" placeholder=\"请输入用户\" suffix-icon=\"el-icon-user\"></el-input>\n          <el-input style=\"width: 140px; margin-left: 5px\" placeholder=\"请输入餐品\" suffix-icon=\"el-icon-food\"></el-input>\n          <el-button style=\"margin-left: 5px\" type=\"primary\">搜素</el-button>\n        </div>\n\n        <div style=\"margin: 10px 0\">\n          <el-button type=\"primary\">新增<i class=\"el-icon-circle-plus-outline\" style=\"margin-left: 5px\"></i></el-button>\n          <el-button type=\"info\">导入<i class=\"el-icon-download\" style=\"margin-left: 5px\"></i></el-button>\n          <el-button type=\"info\">导出<i class=\"el-icon-upload2\" style=\"margin-left: 5px\"></i></el-button>\n        </div>\n\n          <el-table :data=\"tableData\" border: stripe>\n            <el-table-column prop=\"date\" label=\"日期\" width=\"140\">\n            </el-table-column>\n            <el-table-column prop=\"name\" label=\"用户\" width=\"140\">\n            </el-table-column>\n            <el-table-column prop=\"dish\" label=\"餐品\" width=\"340\">\n            </el-table-column>\n\n            <el-table-column label=\"操作\">\n              <template slot-scope=\"scope\">\n                <el-button type=\"primary\"><i class=\"el-icon-view\"></i> </el-button>\n                <el-button type=\"success\"><i class=\"el-icon-edit-outline\"></i></el-button>\n                <el-button type=\"danger\"><i class=\"el-icon-delete\"></i> </el-button>\n              </template>\n            </el-table-column>\n          </el-table>\n\n        <div style=\"padding: 10px 0\">\n          <el-pagination\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\"\n              :current-page=\"currentPage4\"\n              :page-sizes=\"[5, 10, 15, 20]\"\n              :page-size=\"10\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              :total=\"400\">\n          </el-pagination>\n        </div>\n        </el-main>\n\n    </el-container>\n  </el-container>\n</template>\n\n<script>\n\nexport default {\n  name: 'HomeView',\n  data() {\n    const item = {\n      date: '2016-05-02',\n      name: '王小虎',\n      dish: '上海市普陀区金沙江路 1518 弄'\n    };\n    return {\n      tableData: Array(10).fill(item),\n      collapseBtnClass: 'el-icon-s-fold',\n      isCollapse: false,\n      sideWidth:200,\n      logoTextShow:true,\n    }\n  },\n  methods:{\n    collapse(){//点击收缩菜单栏按钮触发\n      this.isCollapse = !this.isCollapse;\n      if (this.isCollapse){//收缩\n        this.sideWidth = 64\n        this.collapseBtnClass = 'el-icon-s-unfold'\n        this.logoTextShow = false\n      }else {//展开\n        this.sideWidth = 200\n        this.collapseBtnClass = 'el-icon-s-fold'\n        this.logoTextShow = true\n      }\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}